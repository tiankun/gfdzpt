	}
	public void setDan_wei(String value) {
		this.dan_wei = value;
	}
  public java.sql.Date getReward_time() {
   	return this.reward_time;
	}
	public void setReward_time(java.sql.Date value) {
		this.reward_time = value;
	}
  public String getZi_liao() {
