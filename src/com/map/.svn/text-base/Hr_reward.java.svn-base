package com.map;

public class Hr_reward{    
    
	private java.lang.Integer id;  //主键
	private java.lang.Integer p_id;  //人员主键
	private String reward_type;  //类型
	private String ming_chen;  //名称
	private String dan_wei;  //单位
	private java.sql.Date reward_time;  //时间
	private String zi_liao;  //相关资料
	private String remark;  //备注
    
  public java.lang.Integer getId() {
   	return this.id;
	}
	public void setId(java.lang.Integer value) {
		this.id = value;
	}
