package com.map;

public class Hr_kaoqin{    
    
	private Long dept_id;  //部门id
	private Long p_id;  //考勤统计人员id(对应于表baseinfo的id列)
	private java.sql.Date inut_date;  //考勤录入时间
	private java.sql.Date attend_date;  //考勤时间
	private String zhuangt;  //考勤表状态（1--待部门经理审批；2--审批通过；3--打回）
	private String yijian;  //考勤审核意见
	private String am;  //上午考勤情况
	private String pm;  //下午考勤情况
	private String amnote;  //上午考勤说明
	private String pmnote;  //下午考勤说明
	private Long id;  //主键id
    
  public Long getDept_id() {
   	return this.dept_id;
	}
	public void setDept_id(Long value) {
		this.dept_id = value;
	}
  public Long getP_id() {
   	return this.p_id;
	}
	public void setP_id(Long value) {
		this.p_id = value;
	}
  public java.sql.Date getInut_date() {
   	return this.inut_date;
	}
	public void setInut_date(java.sql.Date value) {
		this.inut_date = value;
	}
  public java.sql.Date getAttend_date() {
   	return this.attend_date;
	}
	public void setAttend_date(java.sql.Date value) {
		this.attend_date = value;
	}
  public String getZhuangt() {
   	return this.zhuangt;
	}
	public void setZhuangt(String value) {
		this.zhuangt = value;
	}
  public String getYijian() {
   	return this.yijian;
	}
	public void setYijian(String value) {
		this.yijian = value;
	}
  public String getAm() {
   	return this.am;
	}
	public void setAm(String value) {
		this.am = value;
	}
  public String getPm() {
   	return this.pm;
	}
	public void setPm(String value) {
		this.pm = value;
	}
  public String getAmnote() {
   	return this.amnote;
	}
	public void setAmnote(String value) {
		this.amnote = value;
	}
  public String getPmnote() {
   	return this.pmnote;
	}
	public void setPmnote(String value) {
		this.pmnote = value;
	}
  public Long getId() {
   	return this.id;
	}
	public void setId(Long value) {
		this.id = value;
	}

}
