package com.map;

public class Sys_approve {    
    
	private java.lang.Long id;  					//ID
	private java.lang.Long licensorid;  			//授权人ID
	private java.lang.Long approval_departid;  		//审批部门ID
	private String approvaldeparts;  				//审批目标:部门集合
	private String approvalpersons;  				//审批目标:员工集合
	private java.lang.Long approval_rating;  		//审批等级
	private java.lang.Integer precedence_level;  	//优先等级
	private String isapproval_deaprt;  				//是否审批部门
	private String isselfapproval;  				//是否是申请人结束审批 
	private String enable;  						//是否启用
	private java.lang.Long aroupid;  				//组ID
	private String remark;  						//备注
	private String isreceivemsg;					//是否接收短信
	
	public java.lang.Long getId() {
		return id;
	}

	public void setId(java.lang.Long id) {
		this.id = id;
	}

	public java.lang.Long getLicensorid() {
		return licensorid;
	}

	public void setLicensorid(java.lang.Long licensorid) {
		this.licensorid = licensorid;
	}

	public java.lang.Long getApproval_departid() {
		return approval_departid;
	}

	public void setApproval_departid(java.lang.Long approval_departid) {
		this.approval_departid = approval_departid;
	}

	public String getApprovaldeparts() {
		return approvaldeparts;
	}

	public void setApprovaldeparts(String approvaldeparts) {
		this.approvaldeparts = approvaldeparts;
	}

	public String getApprovalpersons() {
		return approvalpersons;
	}

	public void setApprovalpersons(String approvalpersons) {
		this.approvalpersons = approvalpersons;
	}

	public java.lang.Long getApproval_rating() {
		return approval_rating;
	}

	public void setApproval_rating(java.lang.Long approval_rating) {
		this.approval_rating = approval_rating;
	}

	public java.lang.Integer getPrecedence_level() {
		return precedence_level;
	}

	public void setPrecedence_level(java.lang.Integer precedence_level) {
		this.precedence_level = precedence_level;
	}

	public String getIsapproval_deaprt() {
		return isapproval_deaprt;
	}

	public void setIsapproval_deaprt(String isapproval_deaprt) {
		this.isapproval_deaprt = isapproval_deaprt;
	}

	public String getIsselfapproval() {
		return isselfapproval;
	}

	public void setIsselfapproval(String isselfapproval) {
		this.isselfapproval = isselfapproval;
	}

	public String getEnable() {
		return enable;
	}

	public void setEnable(String enable) {
		this.enable = enable;
	}

	public java.lang.Long getAroupid() {
		return aroupid;
	}

	public void setAroupid(java.lang.Long aroupid) {
		this.aroupid = aroupid;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getIsreceivemsg() {
		return isreceivemsg;
	}

	public void setIsreceivemsg(String isreceivemsg) {
		this.isreceivemsg = isreceivemsg;
	}

	@Override
	public String toString() {
		return "Sys_approve [id=" + id + ", 审批人ID==" + licensorid
				+ ", 审批部门ID=" + approval_departid
				+ ", 审批对象：部门集合=" + approvaldeparts + ", 审批对象：人员集合="
				+ approvalpersons + ", 审批等级=" + approval_rating
				+ ", 优先等级=" + precedence_level
				+ ", 是否审批部门=" + isapproval_deaprt + ", 是否启用="
				+ enable + ", 组ID=" + aroupid + ", 备注=" + remark + ", 是否是当事人结束审批="
						+ isselfapproval +  ", 是否接收短信=" + isreceivemsg +  "]";
	}

}