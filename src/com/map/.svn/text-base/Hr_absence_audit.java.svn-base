package com.map;

public class Hr_absence_audit{    
    
	private Long id;  //主键id
	private Long absence_id;  //请假单id
	private Long audit_id;  //审核人id
	private String opinion;  //审核意见
	private String state;  //状态
	private java.sql.Date audit_date;  //审核时间
    
  public Long getId() {
   	return this.id;
	}
	public void setId(Long value) {
		this.id = value;
	}
  public Long getAbsence_id() {
   	return this.absence_id;
	}
	public void setAbsence_id(Long value) {
		this.absence_id = value;
	}
  public Long getAudit_id() {
   	return this.audit_id;
	}
	public void setAudit_id(Long value) {
		this.audit_id = value;
	}
  public String getOpinion() {
   	return this.opinion;
	}
	public void setOpinion(String value) {
		this.opinion = value;
	}
  public String getState() {
   	return this.state;
	}
	public void setState(String value) {
		this.state = value;
	}
  public java.sql.Date getAudit_date() {
   	return this.audit_date;
	}
	public void setAudit_date(java.sql.Date value) {
		this.audit_date = value;
	}

}
