public class ApproveInfoTemp {

	private java.lang.Long id;  //主键ID
	private java.lang.Long departid;  //部门ID
	private java.lang.Long parentid;  //出差管理表ID
	private java.lang.Long personnelid;  //审批人ID
	private java.lang.Integer appstatus;  //审批状态级别
	private String appidear;  //审核意见
	private java.sql.Date apptime;  //审核时间
	private String curstage;  //当前阶段文字描述
	private String undoReason;  //打回原因
	private String invalidReason;  //作废原因
	public java.lang.Long getId() {
		return id;
	}
	public void setId(java.lang.Long id) {
		this.id = id;
	}
	public java.lang.Long getDepartid() {
		return departid;
	}
	public void setDepartid(java.lang.Long departid) {
		this.departid = departid;
	}
	public java.lang.Long getParentid() {
		return parentid;
	}
	public void setParentid(java.lang.Long parentid) {
		this.parentid = parentid;
	}
	public java.lang.Long getPersonnelid() {
		return personnelid;
	}
	public void setPersonnelid(java.lang.Long personnelid) {
		this.personnelid = personnelid;
	}
	public java.lang.Integer getAppstatus() {
		return appstatus;
	}
	public void setAppstatus(java.lang.Integer appstatus) {
		this.appstatus = appstatus;
	}
	public String getAppidear() {
		return appidear;
	}
	public void setAppidear(String appidear) {
		this.appidear = appidear;
	}
	public java.sql.Date getApptime() {
		return apptime;
	}
	public void setApptime(java.sql.Date apptime) {
		this.apptime = apptime;
	}
	public String getCurstage() {
		return curstage;
	}
	public void setCurstage(String curstage) {
		this.curstage = curstage;
	}
	public String getUndoReason() {
		return undoReason;
	}
	public void setUndoReason(String undoReason) {
		this.undoReason = undoReason;
	}
	public String getInvalidReason() {
		return invalidReason;
	}
	public void setInvalidReason(String invalidReason) {
		this.invalidReason = invalidReason;
	}
}
