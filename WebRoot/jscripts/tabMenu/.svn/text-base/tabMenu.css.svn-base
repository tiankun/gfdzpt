#tabMenu li {list-style:none;}
#tabMenu .none {display:none;}

#tabMenu #TabPage1 {height:33px;margin:1px 0 -1px 0;position:relative;z-index:1;}
#tabMenu #TabPage1 li {float:left;margin:0 3px 0 0;padding:12px 0 0 0;width:86px;height:21px;background: url("../../image/tab.gif") no-repeat left 0;
                       cursor:pointer;color:#fff;text-align:center;letter-spacing:3px;}
#tabMenu #TabPage1 li.Selected {padding:10px 0 0 1px;width:86px;height:23px;background: url("../../image/tab.gif") no-repeat left -33px;
                                font-size:14px;font-weight:600;color:#000;}
#tabMenu #cnt {position:relative;z-index:0;border:1px #1A76B7 solid;height:600px!important;height:100%;}
#tabMenu #cnt div {height:100%;}
#tabMenu #cnt div iframe {height:517px!important;height:100%;width:100%;}
#tabMenu #cnt div iframe {height:100%!important;}/* IE7 hack */
#tabMenu #cnt .Box {height:100%; display:none;}
#tabMenu #cnt div#dTab1 {display:block;}
